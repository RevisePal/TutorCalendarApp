{
  "expo": {
    "name": "Kiddl",
    "slug": "Kiddl",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/kiddl-logo.webp",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/kiddl-logo.webp",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kiddl" 
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/kiddl-logo.webp",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kiddl"
    },
    "web": {
      "favicon": "./assets/kiddl-logo.webp"
    }
  }
}
